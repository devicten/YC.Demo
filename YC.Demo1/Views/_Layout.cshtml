<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>
    <link href="https://getbootstrap.com/docs/5.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <link rel="stylesheet" href="@Url.Content($"~/assets/Page.css?t={DateTime.Now.Ticks}")">
      @RenderSection("BLOCK_CSS")
</head>
<body>
    
    
    <main id="PANEL_ERROR" class="container">
      <div class="bg-body-tertiary p-5 rounded mt-3">
        <h1 class="err-title"></h1>
        <p class="err-msg lead"></p>
      </div>
    </main>
    <main id="PANEL_LOADING" class="container">
      <div class="d-flex justify-content-center">
      <div class="spinner-border" role="status">
        <span class="visually-hidden">Loading...</span>
      </div>
    </div>
    </main>
    <main id="PANEL_MAIN">
        @RenderBody()
    </main>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
    <script src="~/assets/jquery.min.js"></script> 
    <script src="@Url.Content($"~/assets/Page.js?t={DateTime.Now.Ticks}")"></script> 
    <script>
    var _URL_INDEX_ = '@Url.Content("~/Login")';
    var _URL_LoginWebApp_ = '@Url.Content("~/Login/WebApp")';
    var _URL_LOBBY_ = '@Url.Content("~/Lobby")';
    var _API_LOGIN_ = '@Url.Content("~/api/Login")';
    var _API_SALES_ = '@Url.Content("~/api/Sales")';
    
    </script>
    @RenderSection("BLOCK_JS")
</body>
</html>